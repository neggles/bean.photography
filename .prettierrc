{
    "plugins": ["prettier-plugin-organize-imports"],
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "printWidth": 100,
    "trailingComma": "all",
    "overrides": [
        { "files": ["*.html"], "options": { "tabWidth": 4 } },
        { "files": ["*.css", "*.scss"], "options": { "tabWidth": 2 } }
    ],
    "organizeImportsSkipDestructiveCodeActions": true
}
